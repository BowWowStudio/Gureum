    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "webpack": "^4.37.0"
  }
