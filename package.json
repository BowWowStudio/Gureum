{
  "name": "locked-cabinet-ui",
  "version": "0.0.0",
  "keywords": [
    "angular",
    "angular 8",
    "firebase",
    "material",
    "nosql",
    "oauth",
    "starter-template",
    "ngx"
  ],
  "repository": "https://github.com/BowWowStudio/LockedCabinet-ui",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reset": "rm -rf ./node_modules ./package-lock.json && npm install",
    "dist": "ng build",
    "prod": "ng build --prod",
    "prod:hashless": "ng build --prod --output-hashing none",
    "prod:src": "ng build --prod --sourcemaps",
    "compodoc:g": "compodoc -p src/tsconfig.app.json",
    "compodoc:s": "compodoc -p src/tsconfig.app.json --serve",
    "compodoc:w": "compodoc -p src/tsconfig.app.json --serve --watch",
    "tscov": "tscov -m 90",
    "tscov:d": "tscov --details"
  },
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.0.3",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/platform-server": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@types/crypto-js": "^3.1.43",
    "@types/file-saver": "^2.0.1",
    "core-js": "^3.1.4",
    "crypto-js": "^3.1.9-1",
    "file-saver": "^2.0.2",
    "firebase": "^6.2.4",
    "hammerjs": "^2.0.8",
    "hash.js": "^1.1.7",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "ts-helpers": "1.1.2",
    "typescript": "^3.4.5",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.3",
    "@angular/cli": "^8.0.3",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@compodoc/compodoc": "^1.1.9",
    "@liftr/tscov": "^1.4.1",
    "@types/jasmine": "^3.3.13",
    "@types/node": "^12.0.10",
    "codelyzer": "^5.1.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "webpack": "^4.35.0"
  }
}
